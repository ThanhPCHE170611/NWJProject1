<Window x:Class="NWJProject1.Views.GroupViews.AddGroup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NWJProject1.Views.GroupViews"
        xmlns:groupformcs="clr-namespace:NWJProject1.Views.UserControls"
        mc:Ignorable="d"
        Title="AddGroup" Height="450" Width="750"
        Background="#333333" 
        Name="addGroupWindow" WindowStyle="ToolWindow">

    <groupformcs:GroupFormUC ButtonContent="Add" ButtonCommand="{Binding AddGroupCommand}" ButtonCommandParameter="{Binding ElementName=addGroupWindow}" IsEditMode="True" />
</Window>
