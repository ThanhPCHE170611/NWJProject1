<Window x:Class="NWJProject1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NWJProject1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnUser" Content="UserHomePage" HorizontalAlignment="Center" VerticalAlignment="Center"
                FontSize="16" Height="60" Click="btnUser_Click"/>
        <Button x:Name="btnGroup" Content="GroupHomePage" HorizontalAlignment="Center" VerticalAlignment="Center"
        FontSize="16" Height="60" Grid.Column="1" Click="btnGroup_Click"/>
    </Grid>
</Window>
